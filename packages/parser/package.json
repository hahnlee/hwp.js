{
  "name": "@hwp.js/parser",
  "version": "0.0.4-alpha.7",
  "type": "module",
  "sideEffect": false,
  "exports": {
    ".": {
      "require": "./src/index.ts",
      "import": "./src/index.ts",
      "types": "./src/index.d.ts"
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "require": {
          "types": "./dist/index.d.cts",
          "default": "./dist/index.cjs"
        },
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.js"
        }
      },
      "./package.json": "./package.json"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepack": "tsup",
    "lint": "eslint ./src",
    "test": "node --import tsx --test **/*.test.ts",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@hwp.js/eslint-config": "workspace:*",
    "@types/aes-js": "^3.1.4",
    "@types/node": "^20",
    "@types/pako": "^1.0.1",
    "eslint": "^8",
    "tsup": "^8.0.2",
    "tsx": "^4.7.2",
    "typescript": "^5"
  },
  "dependencies": {
    "aes-js": "^3.1.2",
    "cfb": "^1.2.2",
    "pako": "^2.1.0"
  }
}
